 			reg = <0x0 0xe6260000 0x0 0x1000>;
 			iccom,reg-offset = <0x1480 0xa484 0x14c0 0xa4e0>;
-			iccom,cta-memory = <0x0 0x37fc9000 0x0 0x2000>;
+			iccom,cta-memory = <0x0 0x37fc7000 0x0 0x4000>;
